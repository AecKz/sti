<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>STI</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>tigre.sti.controlador.IndexController</servlet-name>
		<servlet-class>tigre.sti.controlador.IndexController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tigre.sti.controlador.IndexController</servlet-name>
		<url-pattern>/IndexController</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>tigre.sti.controlador.DashboardController</servlet-name>
		<servlet-class>tigre.sti.controlador.DashboardController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tigre.sti.controlador.DashboardController</servlet-name>
		<url-pattern>/DashboardController</url-pattern>
	</servlet-mapping>
</web-app>
